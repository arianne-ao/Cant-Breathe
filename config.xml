<?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns = "http://www.w3.org/ns/widgets"
        xmlns:gap = "http://phonegap.com/ns/1.0"
        id        = "com.mess.cantbreatheee"
        versionCode="10" 
        version   = "1.0.0">


    <name>Can't Breathe</name>

    <description>
	        Save marine life from inhumanity.
    </description>

    <author href="https://build.phonegap.com" email="support@phonegap.com">
        MESS 
    </author>
	
	<preference name="fullscreen" value="true"/>
 	<preference name="orientation" value="landscape"/>
	<preference name="android-minSdkVersion" value="7" />
    <preference name="android-installLocation" value="auto" />

    <icon src="icon.png" />
    <icon gap:density="ldpi" gap:platform="android" src="icon/icon-ldpi.png" />
    <icon gap:density="mdpi" gap:platform="android" src="icon/icon-mdpi.png" />
    <icon gap:density="hdpi" gap:platform="android" src="icon/icon-hdpi.png" />
    <icon gap:density="xhdpi" gap:platform="android" src="icon/icon-xhdpi.png" />


	<access origin="*" />
	

</widget>